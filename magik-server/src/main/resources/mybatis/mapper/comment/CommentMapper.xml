<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.magik.mapper.comment.CommentMapper">
  <insert id="createComment">
    insert into comment(id,content,userId,mId) values (#{id},#{content},#{userId},#{mId});
  </insert>
</mapper>